      <item>
        <title>Dane Reynolds Layback</title>
        <description>&lt;script&gt;

var images = [];
function preload() {
    for (var i = 0; i &lt; arguments.length; i++) {
        images[i] = new Image();
        images[i].src = preload.arguments[i];
    }
}

//-- usage --//
preload(
    &quot;/images/dane-layback/1.jpg&quot;,
    &quot;/images/dane-layback/2.jpg&quot;,
    &quot;/images/dane-layback/3.jpg&quot;,
    &quot;/images/dane-layback/4.jpg&quot;,
    &quot;/images/dane-layback/5.jpg&quot;,
    &quot;/images/dane-layback/6.jpg&quot;,
    &quot;/images/dane-layback/7.jpg&quot;,
    &quot;/images/dane-layback/8.jpg&quot;,
    &quot;/images/dane-layback/9.jpg&quot;,
    &quot;/images/dane-layback/10.jpg&quot;,
    &quot;/images/dane-layback/12.jpg&quot;,
    &quot;/images/dane-layback/13.jpg&quot;,
    &quot;/images/dane-layback/14.jpg&quot;,
    &quot;/images/dane-layback/15.jpg&quot;,
    &quot;/images/dane-layback/16.jpg&quot;,
    &quot;/images/dane-layback/17.jpg&quot;
)

  // define images
  var images = [

    &quot;/images/dane-layback/1.jpg&quot;,
    &quot;/images/dane-layback/2.jpg&quot;,
    &quot;/images/dane-layback/3.jpg&quot;,
    &quot;/images/dane-layback/4.jpg&quot;,
    &quot;/images/dane-layback/5.jpg&quot;,
    &quot;/images/dane-layback/6.jpg&quot;,
    &quot;/images/dane-layback/7.jpg&quot;,
    &quot;/images/dane-layback/8.jpg&quot;,
    &quot;/images/dane-layback/9.jpg&quot;,
    &quot;/images/dane-layback/10.jpg&quot;,
    &quot;/images/dane-layback/12.jpg&quot;,
    &quot;/images/dane-layback/13.jpg&quot;,
    &quot;/images/dane-layback/14.jpg&quot;,
    &quot;/images/dane-layback/15.jpg&quot;,
    &quot;/images/dane-layback/16.jpg&quot;,
    &quot;/images/dane-layback/17.jpg&quot;
  ];

  // TweenMax can tween any property of any object. We use this object to cycle through the array
  var obj = {curImg: 0};

  // create tweenfile:///C:/Users/Andrew/Desktop/1.png
  var tween = TweenMax.to(obj, 0.5,
    {
      curImg: images.length - 1,  // animate propery curImg to number of images
      roundProps: &quot;curImg&quot;,       // only integers so it can be used as an array index
      repeat: 0,                  // repeat 3 times
      immediateRender: true,      // load first image automatically
      ease: Linear.easeNone,      // show every image the same ammount of time
      onUpdate: function () {
        $(&quot;#myimg&quot;).attr(&quot;src&quot;, images[obj.curImg]); // set the image source
      }
    }
  );

  // init controller
  var controller = new ScrollMagic.Controller();

  // build scene
  var scene = new ScrollMagic.Scene({triggerElement: &quot;#trigger&quot;, duration: 1400})
          .setTween(tween)
          .removeIndicators() // add indicators (requires plugin)
          .addTo(controller);

&lt;/script&gt;

&lt;/head&gt;

&lt;body&gt;

&lt;div style=&quot;height:350vh; position:absolute; width:20px;&quot;&gt;&lt;/div&gt;

&lt;style type=&quot;text/css&quot;&gt;
  @media (max-width: 1000px) {
    #myimg {
      object-position: 25% 50%;
    }
  }  
&lt;/style&gt;

&lt;div id=&quot;imagesequence&quot; id=&quot;trigger&quot;&gt;
  &lt;img id=&quot;myimg&quot; class=&quot;hover_effect&quot; src=&quot;/images/dane-layback/1.jpg&quot;&gt;
&lt;/div&gt;

&lt;div class=&quot;bottommenu_wrap hideit&quot;&gt;
  &lt;p class=&quot;bottommenu_text&quot;&gt;
    &lt;a target=&quot;_blank&quot; href=&quot;https://instagram.com/sealtooth&quot;&gt;@sealtooth&lt;/a&gt; for &lt;a target=&quot;_blank&quot; href=&quot;https://chapter11.tv&quot;&gt;CH11&lt;/a&gt;
  &lt;/p&gt;
&lt;/div&gt;

&lt;div class=&quot;photog_wrap hideit&quot;&gt;
  &lt;p class=&quot;photog_text&quot;&gt;
    cr &lt;a target=&quot;_blank&quot; href=&quot;https://instagram.com/miniblanchard&quot;&gt;@miniblanchard&lt;/a&gt;
  &lt;/p&gt;
&lt;/div&gt;</description>
